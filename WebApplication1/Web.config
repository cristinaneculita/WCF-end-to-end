  </entityFramework>
  <system.serviceModel>
    <services>
      <service name="GeoLib.Services.GeoManager">
        <endpoint address="" binding="wsHttpBinding" contract="GeoLib.Contracts.IGeoService" />
      </service>
    </services>
    <serviceHostingEnvironment>
      <serviceActivations>
        <add service="GeoLib.Services.GeoManager"
             relativeAddress="GeoService.svc"
             factory="GeoLib.WebHost.CustomHostFactory"/>
        
      </serviceActivations>
    </serviceHostingEnvironment>
    
    
    
